@model HotelsBooking.Models.AppProfile.AllProfilesViewModel

<h1>Profiles</h1>

<table class="table table-hover" id="profileIndexTable">
    <tbody>
        @foreach(var profile in Model.profilesList)
        {
            <tr>
                <td>
                    @profile.FirstName
                </td>
                <td>
                    @profile.LastName
                </td>
            </tr>
        }
    </tbody>
</table>